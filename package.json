{
  "name": "coach-me-monorepo",
  "private": true,
  "workspaces": [
    "coach_me_back",
    "front_web"
  ],
  "scripts": {
    "dev:front": "npm run dev --workspace=front_web",
    "dev:back": "npm run dev --workspace=coach_me_back",
    "dev": "npm-run-all --parallel dev:*",
    "install:all": "npm install"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}